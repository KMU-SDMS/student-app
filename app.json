{
  "expo": {
    "name": "SDS",
    "slug": "sds",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sds",
    "userInterfaceStyle": "automatic",
    "platforms": ["ios", "android", "web"],
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {}
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.minseok.sds"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "name": "SDS",
      "shortName": "SDS",
      "description": "A short description of your app.",
      "lang": "ko",
      "themeColor": "#ffffff",
      "backgroundColor": "#ffffff",
      "display": "standalone"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "extra": {},
    "experiments": {
      "typedRoutes": true
    }
  }
}
